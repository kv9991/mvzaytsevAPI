{
  "extends": "airbnb",
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "rules": {
    "no-use-before-define": 0,
    "no-shadow": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id", "__v"]
      }
    ],
    "jsx-a11y/href-no-hash": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "no-else-return": 0,
    "no-tabs": 0,
    "comma-dangle": 0,
    "global-require": 0,
    "indent": 1,
    "no-unederscore-dangle": 0,
    "import/prefer-default-export": 0,
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": ["error", "never", { "svg": "always" }],
    "array-callback-return": 0,
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-mixed-operators": 0
  }
}
